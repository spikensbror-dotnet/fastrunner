<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>spikensbror</Authors>
    <Description>FastRunner is an open-source, lightweight background job engine for fast-executing background jobs scheduled using TimeSpan intervals.</Description>
    <PackageProjectUrl>https://github.com/spikensbror-dotnet/fastrunner</PackageProjectUrl>
    <RepositoryUrl>https://github.com/spikensbror-dotnet/fastrunner</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

</Project>
